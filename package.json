    "storybook": "^8.0.0",
    "style-dictionary": "^4.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.16.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^2.1.0"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
}
}
