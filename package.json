{
  "name": "qa-agile-guide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run tokens:build && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "tokens:build": "style-dictionary build --config style-dictionary.config.mjs || echo 'Tokens build skipped'",
    "tokens:sync": "node scripts/sync-figma-tokens.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "setup:figma": "node scripts/setup-figma-integration.js",
    "generate-icons": "node scripts/generate-pwa-icons.js"
  },
  "dependencies": {
    "@google/genai": "^1.29.1",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/dompurify": "^3.0.5",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.0.6",
    "docx": "^8.5.0",
    "dompurify": "^3.0.6",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.1.0",
    "lucide-react": "^0.400.0",
    "mammoth": "^1.6.0",
    "marked": "^17.0.0",
    "openai": "^4.28.0",
    "pdf-lib": "^1.17.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.15.4",
    "usehooks-ts": "^3.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
},
  "devDependencies": {
    "@storybook/addon-designs": "^7.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "^5.5.10",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^1.4.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "chromatic": "^11.2.0",
    "daisyui": "^4.12.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.0",
    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^5.11.0",
